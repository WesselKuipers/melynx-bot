{
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["node"],
    "esModuleInterop": true,
    "jsx": "react",
    "module": "CommonJS",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./build/",
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "ES6"
  },
  "exclude": ["node_modules"]
}
