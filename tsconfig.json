{
  "compilerOptions": {
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "CommonJS",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./build/",
    "removeComments": true,
    "target": "es2020",
    "noEmit": true,
    "skipLibCheck": true
  }
}
